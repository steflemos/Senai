DROP DATABASE IF EXISTS DBEXERCICIO07;
CREATE DATABASE DBEXERCICIO07;
USE DBEXERCICIO07;
CREATE TABLE ELEITOR (
	IDALUNO INT NOT NULL AUTO_INCREMENT,
	NOME VARCHAR(100),
	DATA_DE_NASCIMENTO INT,
    CIDADE VARCHAR(100),
    TELEFONE_CEL INT,
    TELEFONE_RES INT,
    TELEFONE_CONT VARCHAR(100),
    ENDERECO_COMPLETO VARCHAR(150),
    CEP INT,
	PRIMARY KEY (IDALUNO)
);